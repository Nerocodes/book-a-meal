{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "book a meal api",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./backend -s",
    "build": "npm run clean && npm run build-server",
    "start": "node ./build/index.js",
    "dev-start": "nodemon --exec babel-node ./backend/index.js",
    "test": "set TEST_ENV=test && nyc mocha --require @babel/register ./backend/test --exit || true",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nerocodes/book-a-meal.git"
  },
  "author": "Nerocodes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nerocodes/book-a-meal/issues"
  },
  "homepage": "https://github.com/Nerocodes/book-a-meal#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "dotenv": "^6.2.0",
    "eslint-plugin-import": "^2.16.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "mocha": "^6.0.1",
    "pg": "^7.8.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.43.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "eslint": "^5.14.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  }
}
